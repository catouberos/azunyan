{
  "name": "azunyan",
  "description": "Tana.moe's Discord utilities bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file .env dist/index.js",
    "lint": "biome lint .",
    "format": "biome format .",
    "prepare": "husky"
  },
  "author": "catou",
  "license": "ISC",
  "type": "module",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --apply --organize-imports-enabled=true --no-errors-on-unmatched"
    ]
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.6",
    "@iamtraction/google-translate": "github:iamtraction/google-translate",
    "@types/turndown": "^5.0.4",
    "consola": "^3.2.3",
    "discord-api-types": "^0.37.73",
    "discord-player": "^6.6.7",
    "discord.js": "^14.14.1",
    "mediaplex": "^0.0.9",
    "ofetch": "^1.3.3",
    "play-dl": "^1.9.7",
    "tesseract.js": "^5.0.5",
    "turndown": "^7.1.2",
    "ufo": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/node": "^20.11.25",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "volta": {
    "node": "20.11.0"
  },
  "trustedDependencies": ["@discordjs/opus", "mediaplex"]
