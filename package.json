{
  "name": "azunyan",
  "description": "Tana.moe's Discord utilities bot",
  "main": "index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20.14.0 --format=esm --packages=external --outfile=build/app.js",
    "start": "node --env-file .env build/app.js",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "prepare": "husky"
  },
  "author": "catou",
  "license": "ISC",
  "type": "module",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --apply --organize-imports-enabled=true --no-errors-on-unmatched"
    ]
  },
  "engines": {
    "node": ">=20.14.0"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.5.1",
    "@iamtraction/google-translate": "github:iamtraction/google-translate",
    "@types/turndown": "^5.0.5",
    "cheerio": "^1.0.0",
    "consola": "^3.2.3",
    "discord-api-types": "^0.37.101",
    "discord-player": "^6.7.1",
    "discord-player-youtubei": "^1.3.2",
    "discord.js": "^14.16.2",
    "mediaplex": "^1.0.0",
    "ofetch": "^1.4.0",
    "subsonic-api": "^3.0.0",
    "turndown": "^7.2.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^20.16.10",
    "esbuild": "^0.24.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@discordjs/opus",
    "esbuild",
    "mediaplex"
  ]
}
