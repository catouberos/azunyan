{
  "name": "azunyan",
  "description": "Tana.moe's Discord utilities bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file .env dist/index.js",
    "lint": "biome lint .",
    "format": "biome format .",
    "postinstall": "husky install"
  },
  "author": "catou",
  "license": "ISC",
  "type": "module",
  "lint-staged": {
    "**.js|ts|cjs|mjs|d.cts|d.mts|jsx|tsx|json|jsonc": [
      "biome check --files-ignore-unknown=true",
      "biome check --apply --no-errors-on-unmatched",
      "biome check --apply --organize-imports-enabled=false --no-errors-on-unmatched",
      "biome check --apply-unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --apply --no-errors-on-unmatched"
    ],
    "*": [
      "biome check --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.5",
    "@iamtraction/google-translate": "github:iamtraction/google-translate",
    "@types/turndown": "^5.0.4",
    "consola": "^3.2.3",
    "discord-api-types": "^0.37.62",
    "discord-player": "^6.6.6",
    "discord.js": "^14.13.0",
    "mediaplex": "^0.0.9",
    "play-dl": "^1.9.7",
    "tesseract.js": "^5.0.4",
    "turndown": "^7.1.2",
    "ufo": "^1.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.1",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/node": "^20.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.10.0",
    "pnpm": "8.11.0"
  },
  "trustedDependencies": [
    "@discordjs/opus",
    "mediaplex"
  ]
}
